{
  "name": "PhyLab",
  "version": "1.0.2",
  "description": "This is PhyLab desktop application",
  "repository": "pamrulla/phylab_supportfiles",
  "main": "main.js",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^2.0.1"
